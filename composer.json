{
  "name": "denniscuijpers/laravel-global-id",
  "description": "Laravel Global ID package",
  "homepage": "https://github.com/denniscuijpers/laravel-global-id",
  "keywords": [
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis Cuijpers",
      "email": "dennis@redacted.com"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "illuminate/support": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "DennisCuijpers\\GlobalId\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DennisCuijpers\\GlobalId\\GlobalIdServiceProvider"
      ],
      "aliases": {
        "GlobalId": "DennisCuijpers\\GlobalId\\Facades\\GlobalId"
      }
    }
  }
}
